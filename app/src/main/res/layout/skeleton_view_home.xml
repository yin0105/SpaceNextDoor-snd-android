<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginEnd="14dp"
    android:layout_marginStart="14dp"
    android:layout_marginBottom="8dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll"
            android:layout_width="wrap_content"

            android:layout_height="match_parent"
            android:background="@drawable/skeleton_view_round"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:orientation="horizontal"
                android:weightSum="2">

                <View
                    android:id="@+id/tv_title"
                    android:layout_width="0dp"
                    android:layout_height="16dp"
                    android:layout_weight="0.8"
                    android:background="@drawable/access_skeleton_bg" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:orientation="horizontal"
                android:weightSum="2">

                <View
                    android:id="@+id/tv_title1"
                    android:layout_width="0dp"
                    android:layout_height="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_weight="0.8"
                    android:background="@drawable/access_skeleton_bg" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="12dp"
                android:orientation="horizontal"
                android:weightSum="2">

                <View
                    android:id="@+id/tv_title111"
                    android:layout_width="0dp"
                    android:layout_height="14dp"
                    android:layout_weight="1.1"
                    android:background="@drawable/skeleton_color_background" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:orientation="horizontal"
                android:weightSum="2">

                <View
                    android:id="@+id/tv_title1111"
                    android:layout_width="0dp"
                    android:layout_height="14dp"
                    android:layout_marginTop="8dp"
                    android:layout_weight="1.1"
                    android:background="@drawable/skeleton_color_background" />
            </LinearLayout>

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:alpha="2"
                android:background="@color/txtview_round_color" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginStart="18dp"
                android:layout_marginTop="17dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/view2">

                    <LinearLayout
                        android:id="@+id/ll11"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="1">

                        <View
                            android:id="@+id/tv_content44"
                            android:layout_width="0dp"
                            android:layout_height="12dp"
                            android:layout_weight="0.4"
                            android:background="@drawable/access_skeleton_bg" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ll11"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        android:weightSum="1">

                        <View
                            android:id="@+id/tv_content3"
                            android:layout_width="0dp"
                            android:layout_height="10dp"
                            android:layout_weight="0.6"
                            android:background="@drawable/skeleton_color_background" />


                    </LinearLayout>

                </RelativeLayout>


                <View
                    android:id="@+id/view2"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/skeleton_color_background" />


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/view2">

                    <LinearLayout
                        android:id="@+id/ll1111"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="1">

                        <View
                            android:id="@+id/tv_content441"
                            android:layout_width="0dp"
                            android:layout_height="12dp"
                            android:layout_weight="0.4"
                            android:background="@drawable/access_skeleton_bg" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll111"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ll1111"
                        android:layout_marginTop="10dp"
                        android:orientation="horizontal"
                        android:weightSum="1">

                        <View
                            android:id="@+id/tv_content34"
                            android:layout_width="0dp"
                            android:layout_height="10dp"
                            android:layout_weight="0.7"
                            android:background="@drawable/skeleton_color_background" />
                    </LinearLayout>
                </RelativeLayout>

            </RelativeLayout>

            <View
                android:id="@+id/viewLine"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                android:alpha="2"
                android:background="@color/txtview_round_color" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="25dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:weightSum="1">

                <View
                    android:id="@+id/tv_title_1"
                    android:layout_width="0dp"
                    android:layout_height="17dp"
                    android:layout_weight="0.4"
                    android:background="@drawable/access_skeleton_bg" />

                <View
                    android:id="@+id/tv_title_2"
                    android:layout_width="0dp"
                    android:layout_height="12dp"
                    android:layout_marginStart="60dp"
                    android:layout_weight="0.5"
                    android:background="@drawable/access_skeleton_bg" />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="25dp"
            android:gravity="center"
            android:layout_height="wrap_content">

            <View
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:background="@drawable/circle_view_gray" />

            <View
                android:layout_width="10dp"
                android:layout_marginStart="7dp"
                android:layout_height="10dp"
                android:background="@drawable/circle_view_light_grey" />

        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <View
                android:id="@+id/tv_titlee_1"
                android:layout_width="0dp"
                android:layout_height="16dp"
                android:layout_weight="0.6"
                android:background="@drawable/skeleton_color_background" />

            <View
                android:id="@+id/tv_titlee_11"
                android:layout_width="0dp"
                android:layout_height="16dp"
                android:layout_weight="0.2" />

            <View
                android:id="@+id/tv_titlee_2"
                android:layout_width="0dp"
                android:layout_height="12dp"
                android:layout_marginEnd="25dp"
                android:layout_weight="0.26"
                android:background="@drawable/skeleton_color_background" />

        </LinearLayout>


    </LinearLayout>



</RelativeLayout>